    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "next": "13.0.4",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "swr": "2.0.0-rc.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "prisma": "^4.6.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
